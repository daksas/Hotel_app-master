<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dak.hotel_app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Contact"
            android:label="@string/title_activity_contact" >
        </activity>
        <activity
            android:name=".Rooms"
            android:label="@string/title_activity_rooms" >
        </activity>
        <activity
            android:name=".Reservation_form"
            android:label="@string/title_activity_reservation_form" >
        </activity>
        <activity
            android:name=".Luxury_room"
            android:label="@string/title_activity_luxury_room" >
        </activity>
        <activity
            android:name=".Basic_room"
            android:label="@string/title_activity_basic_room" >
        </activity>
    </application>

</manifest>
